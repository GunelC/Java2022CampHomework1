package array2Odev1;

public class Main {

	public static void main(String[] args) {
 
	String[][] sehirler = new String[3][3];
    
	sehirler[0][0] = "İstanbul";
	sehirler[0][1] = "Bursa";
	sehirler[0][2] = "Bilecik";
	sehirler[1][0] = "Ankara";
	sehirler[1][1] = "Konya";
	sehirler[1][2] = "Kayseri";
	sehirler[2][0] = "Diyarbakır";
	sehirler[2][1] = "Şanlıurfa";
	sehirler[2][2] = "Gaziantep";
	
	for(int i = 0; i<=2; i++) {
		for(int j = 0; j <= 2; j++) {
			
			System.out.println(sehirler[i][j]);
			
		}
		System.out.println(" -------  ");
	}
	
	
	///////////
	
	
	int[][] sayi = new int[3][3];
	
	sayi[0][0] = 2 ;
	sayi[0][1] = 45 ;
	sayi[0][2] = 67;
	sayi[1][0] = 33;
	sayi[1][1] = 5;
	sayi[1][2] = 87;
	sayi[2][0] = 67;
	sayi[2][1] = 17;
	sayi[2][2] = 0;
	
	for(int i = 0; i <= 2; i++) {
		for(int j = 0; j <= 2; j++) {
			
			System.out.println(" "+ sayi[i][j]);
			//System.out.println(" ");
		}
	}
	
	
	
	
	
	}

}
