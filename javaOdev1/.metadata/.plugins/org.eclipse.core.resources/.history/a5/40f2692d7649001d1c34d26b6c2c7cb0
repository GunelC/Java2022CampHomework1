package datatypes;

public class Main {

	public static void main(String[] args) {
	
		//byte, short, int, long tamsayı tutar
		int sayi = 1256878900;
		byte sayi1 =13;
		sayi1 = 127;
		
		//double ve float ondalıklı sayı tutar
		double sayi3 = 12.5;
		
		char karakter = 'G';
		String sehir = "İzmir";
		
		boolean dogruMu = true;
		boolean varMi = false;
	}

}
